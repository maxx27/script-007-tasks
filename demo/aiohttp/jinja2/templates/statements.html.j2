<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Hello</title>
  </head>
  <body>
    <div>
      <h1>
        Hello {{ params['name'] if 'name' in params else 'Anonymous!' }}!
      </h1>
      <div>Users:</div>
      <table>
        <tr>
          <td>Username</td>
          <td>Is active</td>
          <td>Password</td>
        </tr>
        {% for user in users %}
        <tr>
          <td>{{user.name}}</td>
          <td>{{user.is_active}}</td>
          <td>{{user.password}}</td>
        </tr>
        {% endfor %}
      </table>
    </div>
  </body>
</html>
